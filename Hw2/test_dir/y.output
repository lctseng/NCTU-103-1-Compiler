文法

    0 $accept: program $end

    1 program: declaration_list funct_def decl_and_def_list
    2        | declaration_list proc_def decl_and_def_list
    3        | proc_def decl_and_def_list
    4        | funct_def decl_and_def_list

    5 decl_and_def_list: decl_and_def_list declaration_list
    6                  | decl_and_def_list definition_list
    7                  | %empty

    8 declaration_list: declaration_list const_decl
    9                 | declaration_list var_decl
   10                 | declaration_list funct_decl
   11                 | declaration_list proc_decl
   12                 | const_decl
   13                 | var_decl
   14                 | funct_decl
   15                 | proc_decl

   16 var_decl: type var_list SEMICOLON

   17 type: INT
   18     | BOOL
   19     | DOUBLE
   20     | FLOAT
   21     | STRING

   22 identifier: scalar_id
   23           | scalar_id array_indice

   24 var_list: var_entry
   25         | var_entry COMMA var_list

   26 var_entry: identifier
   27          | scalar_id ASSIGN expr
   28          | scalar_id array_indice ASSIGN '{' expr_list '}'

   29 scalar_id: ID

   30 array_indice: single_array_indice array_indice
   31             | single_array_indice

   32 single_array_indice: '[' INTEGER_CONSTANT ']'

   33 const_decl: CONST type const_list SEMICOLON

   34 const_list: single_const
   35           | single_const COMMA const_list

   36 single_const: scalar_id ASSIGN literal_constant

   37 literal_constant: INTEGER_CONSTANT
   38                 | FLOATING_CONSTANT
   39                 | BOOLEAN_CONSTANT
   40                 | STRING_CONSTANT

   41 $@1: %empty

   42 funct_decl: type scalar_id '(' args ')' $@1 SEMICOLON

   43 $@2: %empty

   44 proc_decl: VOID scalar_id '(' args ')' $@2 SEMICOLON

   45 definition_list: definition_list funct_def
   46                | definition_list proc_def
   47                | funct_def
   48                | proc_def

   49 $@3: %empty

   50 funct_def: type scalar_id '(' args ')' $@3 compound_stat

   51 $@4: %empty

   52 proc_def: VOID scalar_id '(' args ')' $@4 compound_stat

   53 args: arg_list
   54     | %empty

   55 $@5: %empty

   56 single_arg: type $@5 identifier

   57 $@6: %empty

   58 single_arg: CONST type $@6 identifier

   59 arg_list: single_arg COMMA arg_list
   60         | single_arg

   61 compound_stat: '{' local_decl_list zero_or_more_stat_list '}'

   62 local_decl_list: var_decl_list
   63                | %empty

   64 var_decl_list: var_decl_entry var_decl_list
   65              | var_decl_entry

   66 var_decl_entry: var_decl
   67               | const_decl

   68 zero_or_more_stat_list: stat_list
   69                       | %empty

   70 stat_list: stat_list stat
   71          | stat

   72 stat: compound_stat
   73     | simple_stat
   74     | if_stat
   75     | while_stat
   76     | for_stat
   77     | jump_stat
   78     | funct_call SEMICOLON

   79 if_stat: if_only_stat
   80        | if_else_stat

   81 if_only_stat: KW_IF '(' bool_expr ')' compound_stat

   82 if_else_stat: KW_IF '(' bool_expr ')' compound_stat KW_ELSE compound_stat

   83 while_stat: KW_WHILE '(' bool_expr ')' compound_stat
   84           | KW_DO compound_stat KW_WHILE '(' bool_expr ')' SEMICOLON

   85 for_stat: KW_FOR '(' for_expr SEMICOLON for_expr SEMICOLON for_expr ')' '{' zero_or_more_stat_list '}'

   86 for_expr: expr
   87         | one_or_more_for_assignment
   88         | %empty

   89 one_or_more_for_assignment: var_ref ASSIGN expr
   90                           | one_or_more_for_assignment COMMA var_ref ASSIGN expr

   91 jump_stat: KW_RETURN expr SEMICOLON
   92          | KW_BREAK SEMICOLON
   93          | KW_CONTINUE SEMICOLON

   94 simple_stat: var_ref ASSIGN expr SEMICOLON
   95            | KW_PRINT expr SEMICOLON
   96            | KW_READ var_ref SEMICOLON

   97 var_ref: scalar_id
   98        | scalar_id intexpr_array_indice

   99 intexpr_array_indice: intexpr_single_array_indice intexpr_array_indice
  100                     | intexpr_single_array_indice

  101 intexpr_single_array_indice: '[' int_expr ']'

  102 int_expr: expr

  103 bool_expr: expr

  104 expr: expr '+' expr
  105     | expr '-' expr
  106     | expr '*' expr
  107     | expr '/' expr
  108     | expr '%' expr
  109     | '-' expr
  110     | '(' expr ')'
  111     | expr OP_LT expr
  112     | expr OP_LE expr
  113     | expr OP_GT expr
  114     | expr OP_GE expr
  115     | expr OP_NE expr
  116     | expr OP_EQ expr
  117     | expr OP_AND expr
  118     | expr OP_OR expr
  119     | OP_NOT expr
  120     | literal_constant
  121     | funct_call
  122     | var_ref

  123 funct_call: scalar_id '(' zero_or_more_expr_list ')'

  124 zero_or_more_expr_list: expr_list
  125                       | %empty

  126 expr_list: expr
  127          | expr_list COMMA expr


